type parameter = unit;
type storage = nat;

type return_ = [list<operation>, storage];

let main = ([_action, store]: [parameter, storage]): return_ => {

  let store_ = store;

  if (Tezos.amount > (0 as tez)) {
    store_ = store + (1 as nat);
  }
 
  return [
    (list([]) as list<operation>),
    store_
  ];
}
